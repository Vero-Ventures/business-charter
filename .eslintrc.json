{
  "globals": {
    "React": "true"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals"
  ],
  "ignorePatterns": [
    "node_modules/",
    "dist/",
    "*.json",
    "*.html",
    "*.css",
    "*.md"
    // Add other file types you want to ignore
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn"
  }
}
